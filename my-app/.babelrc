{
  
  "env": {
    "production": {
       "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals":  ["module"]
          }]
        }
      }
    }
  }
}
